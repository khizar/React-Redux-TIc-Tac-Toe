{
  "name": "TicTacToe",
  "version": "1.0.0",
  "description": "Tic Tac Toe Web-based Game",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "main": "main.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint src test webpack.config.js",
    "stylelint": "./node_modules/.bin/stylelint \"styles/**/[^_]*.scss\"",
    "lint": "npm run eslint && npm run stylelint",
    "start": "node run",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 1 --branches 1 --functions 1 --lines 1",
    "test": "ava --tap | tap-nyan",
    "test:watch": "./node_modules/.bin/ava --watch",
    "cover:watch": "nodemon --quiet --watch test --exec nyc ava -s",
    "clean": "node run clean && rm -rf node_modules && rm -rf .nyc_output && rm -rf coverage"
  },
  "keywords": [],
  "author": "Ioannis Angelogiannis",
  "license": "ISC",
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.7",
    "ava": "^0.16.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "browser-sync": "^2.13.0",
    "connect-history-api-fallback": "^1.2.0",
    "css-loader": "^0.25.0",
    "del": "^2.2.1",
    "ejs": "^2.5.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.4.2",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "nyc": "^8.3.2",
    "pixrem": "^3.0.1",
    "pleeease-filters": "^3.0.0",
    "postcss": "^5.1.1",
    "postcss-calc": "^5.3.0",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-selector-matches": "^2.0.1",
    "postcss-selector-not": "^2.0.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "stylelint-webpack-plugin": "^0.3.1",
    "tap-nyan": "0.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/test_helper.js"
    ],
    "source": [
      "src/**/*.js",
      "src/**/*.jsx",
      "/**/*.jsx",
      "!public/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": false,
    "require": [
      "./test/test_helper.js",
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "extension": [
      ".jsx",
      ".es6"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "immutable": "^3.8.1",
    "lodash": "^4.16.4",
    "qs": "^6.2.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.6.1"
  }
}
