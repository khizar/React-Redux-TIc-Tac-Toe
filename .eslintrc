{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "skipBlankLines": 0,
    "no-console": 0,
    "global-require": 0,
    "jsx-indent": 0,
    "comma-dangle": [
      "error",
      "never"
    ],
    "react/self-closing-comp": ["error", {
      "component": true,
      "html": false
    }],
    "react/require-extension": 0,
    "react/jsx-space-before-closing": 0,
    "react/jsx-indent": [
      2,
      4
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "max-len": [
      "error",
      120
    ],
    "react/jsx-indent-props": [
      2,
      4
    ],
    "react/prefer-stateless-function": 0
  },
  "settings": {
    "import/parser": "babel-eslint"
  },
  "globals": {
    "window": true,
    "location": true,
    "document": true,
    "localStorage": true,
    "sessionStorage": true,
    "fetch": true
  }
}